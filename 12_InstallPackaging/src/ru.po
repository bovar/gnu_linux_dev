#: guesser.c:51
#, c-format
msgid "Usage ./guesser [options]\n"
msgstr "Запуск ./guesser [опции]\n"

#: guesser.c:52
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: guesser.c:53
#, c-format
msgid "\t-r\tUse Roman numbers.\n"
msgstr "\t-r\tИспользовать римские числа.\n"

#: guesser.c:54
#, c-format
msgid "\t--help\tDisplay this information.\n"
msgstr "\t--help\tВывести эту информацию.\n"

#: guesser.c:59
#, c-format
msgid "Guess the number from I to C\n"
msgstr "Загадайте число от I до C\n"

#: guesser.c:61
#, c-format
msgid "Guess the number from 1 to 100\n"
msgstr "Загадайте число от 1 до 100\n"

#: guesser.c:68
#, c-format
msgid "Is your number greater than %s? (yes/no)\n"
msgstr "Ваше число больше чем %s? (да/нет)\n"

#: guesser.c:70
#, c-format
msgid "Is your number greater than %d? (yes/no)\n"
msgstr "Ваше число больше чем %d? (да/нет)\n"

#: guesser.c:75
msgid "yes"
msgstr "да"

#: guesser.c:77
msgid "no"
msgstr "нет"

#: guesser.c:80
#, c-format
msgid "Wrong answer. Say \"yes\" or \"no\"\n"
msgstr "Неправильный формат ответа. Введите \"да\" или \"нет\"\n"

#: guesser.c:84
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число %s\n"

#: guesser.c:86
#, c-format
msgid "Your number is %d\n"
msgstr "Ваше число %d\n"

#~ msgid "No"
#~ msgstr "Нет"
